{
  "name": "vim-flash",
  "displayName": "Vim Flash",
  "description": "Flash jump to anywhere in the editor",
  "version": "0.0.3",
  "publisher": "youguanxinqing",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/youguanxinqing/vim-flash"
  },
  "engines": {
    "vscode": "^1.97.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vim-flash.jump",
        "title": "Vim Flash: jump"
      },
      {
        "command": "vim-flash.cancel",
        "title": "Vim Flash: cancel"
      },
      {
        "command": "vim-flash.deleteChar",
        "title": "Vim Flash: delete character"
      }
    ],
    "keybindings": [
      {
        "key": "escape",
        "command": "vim-flash.cancel",
        "when": "editorTextFocus && vim-flash.active"
      },
      {
        "key": "backspace",
        "command": "vim-flash.deleteChar",
        "when": "editorTextFocus && vim-flash.active"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.97.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "esbuild": "^0.25.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  },
  "extensionKind": [
    "ui"
  ]
}
